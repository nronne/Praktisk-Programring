set term svg
set out "Plot_Rutherford_Soddy_unc.svg"
set xlabel "t"
set ylabel "activity"
set grid
set tics out
set key top right
set termoption dash
plot [0.75:14][] "outwunc.txt" using 1:2 w l title "best fit" \
     , "outwunc.txt" using 1:3 with lines dt 3 lc rgb "black" title "+sigma fit" \
     , "outwunc.txt" using 1:4 with lines dt 3 lc rgb "black" title "-sigma fit" \
     , "data.txt" w yerr pt 7 ps 0.4 title "data" 